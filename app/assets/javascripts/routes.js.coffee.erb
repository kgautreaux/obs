obsApp = angular.module("Obs", ['mobile-navigate', 'ngCookies', 'ngResource'], ($routeProvider, $locationProvider) ->
  $routeProvider.when("/index",
      controller: 'ObsController',
      templateUrl: '<%= Rails.application.routes.url_helpers.templates_index_path %>'
  ).when("/edit/:id",
      controller: 'ObsController',
      templateUrl: '<%= Rails.application.routes.url_helpers.templates_edit_path %>'
  ).when("/new",
      controller: 'ObsController',
      templateUrl: '<%= Rails.application.routes.url_helpers.templates_new_path %>'
  ).otherwise({redirectTo: '/index'})
)
